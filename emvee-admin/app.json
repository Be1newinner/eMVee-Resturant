{
  "expo": {
    "name": "emvee-admin",
    "slug": "emvee-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shipsar.emveeadmin",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router", "expo-image-picker"],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
