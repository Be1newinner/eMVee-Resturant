{
  "expo": {
    "name": "eMVee Restaurant",
    "slug": "emvee-restaurant",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shipsar.emveerestaurant"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shipsar.emveerestaurant",
      "versionCode": 10
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "36e9e9e7-a091-4c30-97d9-9654bd817183"
      }
    }
  }
}
